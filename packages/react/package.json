{
  "name": "@trpc/react",
  "version": "9.24.0",
  "description": "tRPC React lib",
  "author": "KATT",
  "license": "MIT",
  "homepage": "https://trpc.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trpc/trpc.git",
    "directory": "packages/react"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "ssg.ts"
    ]
  },
  "scripts": {},
  "main": "dist/trpc-react.cjs.js",
  "module": "dist/trpc-react.esm.js",
  "typings": "dist/trpc-react.cjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/trpc-react.esm.js",
      "require": "./dist/trpc-react.cjs.js",
      "types": "./dist/trpc-react.cjs.d.ts"
    },
    "./ssg": {
      "import": "./ssg/dist/trpc-react-ssg.esm.js",
      "require": "./ssg/dist/trpc-react-ssg.cjs.js",
      "types": "./ssg/dist/trpc-react-ssg.cjs.d.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "ssg",
    "src"
  ],
  "eslintConfig": {
    "rules": {
      "react-hooks/exhaustive-deps": "error"
    }
  },
  "peerDependencies": {
    "@trpc/client": "^9.12.0",
    "@trpc/server": "^9.12.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-query": "^3.37.0 || ^4.0.0-alpha.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.0"
  },
  "devDependencies": {
    "@trpc/client": "^9.24.0",
    "@trpc/server": "^9.24.0",
    "@types/express": "^4.17.12",
    "express": "^4.17.1",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.37.0",
    "zod": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
