  "devDependencies": {
    "@tanstack/react-query": "^5.80.3",
    "@trpc/client": "11.9.0",
    "@trpc/react-query": "11.9.0",
    "@trpc/server": "11.9.0",
    "@trpc/tanstack-react-query": "11.9.0",
    "@types/jscodeshift": "17.3.0",
    "@types/node": "^22.13.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
