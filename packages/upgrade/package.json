    "package.json",
    "!**/*.test.*",
    "!**/__tests__"
  ],
  "dependencies": {
    "@bomb.sh/args": "0.3.0",
    "@clack/prompts": "0.10.0",
    "jscodeshift": "17.1.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/jscodeshift": "0.12.0",
    "@types/node": "^22.13.5",
    "bunchee": "6.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": [
    "https://trpc.io/sponsor"
  ]
