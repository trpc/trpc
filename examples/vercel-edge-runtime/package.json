    "edge-runtime": "^2.0.2",
    "esbuild": "^0.17.10",
    "start-server-and-test": "^1.12.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
