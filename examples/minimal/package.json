{
  "name": "examples-minimal",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.20.0"
  },
  "scripts": {
    "build": "tsc",
    "dev:server": "node --watch src/server/index.ts",
    "dev:client": "wait-port 3000 && node --watch src/client/index.ts",
    "dev": "run-p dev:* --print-label",
    "test-dev": "start-server-and-test 'node src/server/index.ts' 3000 'node src/client/index.ts'",
    "test-start": "start-server-and-test 'node dist/server/index.js' 3000 'node dist/client/index.js'"
  },
  "dependencies": {
    "@trpc/client": "npm:@trpc/client",
    "@trpc/server": "npm:@trpc/server",
    "superjson": "^1.12.4",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.12.0",
    "typescript": "^5.9.2",
    "wait-port": "^1.0.1"
  },
  "version": "11.7.2"
}
