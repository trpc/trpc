{
  "name": "examples-fastify-server",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.20.0"
  },
  "scripts": {
    "build": "esbuild src/server/index.ts src/client/index.ts --bundle --packages=external --platform=node --format=esm --outdir=dist --sourcemap",
    "dev:server": "node --watch src/server/index.ts",
    "dev:client": "wait-port 2022 && node --watch src/client/index.ts",
    "dev": "run-p dev:* --print-label",
    "lint": "eslint --cache src",
    "typecheck": "tsc",
    "start:server": "node dist/server/index.js",
    "start:client": "wait-port 2022 && node dist/client/index.js",
    "start": "run-p start:* --print-label",
    "test-dev": "start-server-and-test 'node src/server/index.ts' http-get://localhost:2022 'node src/client/index.ts'",
    "test-start": "start-server-and-test 'node dist/server/index.js' http-get://localhost:2022 'node dist/client/index.js'"
  },
  "dependencies": {
    "@fastify/websocket": "^11.0.0",
    "@trpc/client": "npm:@trpc/client",
    "@trpc/server": "npm:@trpc/server",
    "fastify": "^5.0.0",
    "superjson": "^1.12.4",
    "tslib": "^2.8.1",
    "ws": "^8.0.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/ws": "^8.2.0",
    "esbuild": "^0.17.10",
    "eslint": "^9.26.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.12.0",
    "typescript": "^5.9.2",
    "wait-port": "^1.0.1"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "version": "11.7.2"
}
